      "source": [
        "# Replace None with approprite SQL code\n",
        "q8 = \"\"\"\n",
        "SELECT lastName, firstName, employeeNumber, officeCode\n",
        "FROM employees\n",
        "WHERE officeCode IN (\n",
        "    SELECT officeCode\n",
        "    FROM employees\n",
        "    GROUP BY officeCode\n",
        "    HAVING COUNT(*) < 5\n",
        ")\n",
        ";\n",
        "\"\"\"\n",
        "q8_result = pd.read_sql(q8, conn)\n",
        "q8_result"
